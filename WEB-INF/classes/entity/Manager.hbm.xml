<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="entity.Manager" table="t_manager">
    		<id name="musername" length="30">
    		</id>
    		<property name="mpassword" length="30"></property>
    		<property name="mname" length="50"></property>
    		<property name="mtype" length="1"></property>
    		<property name="goodpower" length="1"></property>
    		<property name="orderpower" length="1"></property>
    		<property name="customerpower" length="1"></property>
    		<property name="workerpower" length="1"></property>
    	</class>
    </hibernate-mapping>